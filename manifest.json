{
  // Required
  "manifest_version": 2,
  "name": "YouTube HTML5",
  "description": "Force YouTube to play HTML5 videos",
  "version": "1.0",
  "content_scripts" : [
  	{
  		"matches": ["http://www.youtube.com/*", "https://www.youtube.com/*"],
  		"js": ["script.js"]
  	}
  ],
  "icons": {
  	"128" : "logo.png",
  	"48" : "logo.png"
  }
}